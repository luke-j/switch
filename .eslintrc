{
	"parser": "babel-eslint",
	"rules": {
		"comma-dangle": [2,"never"],
		"no-cond-assign": [2,"always"],
		"no-constant-condition": 2,
		"no-control-regex": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-empty": 2,
		"no-ex-assign": 2,
		"no-extra-parens": [2,"all"],
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-typeof": 2,
		"no-unexpected-multiline": 2,
		"accessor-pairs": [2,{"getWithoutSet":true,"setWithoutGet":true}],
		"block-scoped-var": 2,
		"consistent-return": 2,
		"curly": [2,"all"],
		"default-case": 2,
		"dot-location": [2,"property"],
		"eqeqeq": [2,"smart"],
		"no-alert": 2,
		"no-caller": 2,
		"no-div-regex": 2,
		"no-else-return": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-fallthrough": 2,
		"no-implied-eval": 2,
		"no-iterator": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-multi-spaces": 2,
		"no-native-reassign": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-new": 2,
		"no-octal-escape": 2,
		"no-octal": 2,
		"no-process-env": 2,
		"no-proto": 2,
		"no-redeclare": [2,{"builtinGlobals":true}],
		"no-return-assign": [2,"always"],
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-useless-call": 2,
		"no-void": 2,
		"no-with": 2,
		"yoda": [2,"never",{}],
		"no-label-var": 2,
		"no-shadow-restricted-names": 2,
		"no-shadow": [0],
		"no-undef-init": 2,
		"no-undef": 2,
		"no-undefined": 2,
		"no-unused-vars": 1,
		"no-use-before-define": [2,"nofunc"],
		"array-bracket-spacing": [2,"never",{}],
		"brace-style": [2,"1tbs",{"allowSingleLine":true}],
		"camelcase": 2,
		"comma-style": 2,
		"consistent-this": 2,
		"indent": [2,"tab", {"SwitchCase": 1}],
		"linebreak-style": 2,
		"new-cap": 2,
		"new-parens": 2,
		"no-trailing-spaces": [2,{"skipBlankLines":true}],
		"object-curly-spacing": [2,"never",{}],
		"arrow-spacing": [2,{"before":true,"after":true}],
		"constructor-super": 2,
		"no-const-assign": 2,
		"no-this-before-super": 2,
		"no-var": 2,
		"prefer-const": 1,
		"prefer-spread": 2,
		"require-yield": 2
	},
	"env": {
		"es6": true,
		"mocha": true,
		"browser": false,
		"node": false
	},
	"globals": {
		"print": false,
		"quit": false,
		"scriptArgs": true,
		"read": false,
		"environment": false,
		"global": false,
		"expect": false,
		"sinon": false
	}
}